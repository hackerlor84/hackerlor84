<script language="VBScript">
Function FromHex(hexStr)
    Dim arr, i, r
    arr = Split(hexStr, " ")
    r = ""
    For i = 0 To UBound(arr)
        If arr(i) <> "" Then
            r = r & Chr(CLng("&H" & arr(i)))
        End If
    Next
    FromHex = r
End Function

